<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".activity.item_activity.start_mode.StartModeActivity">

    <Button
        android:id="@+id/start_standard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="standard模式"/>

    <Button
        android:id="@+id/start_singleTop"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="singleTop模式"/>

    <Button
        android:id="@+id/start_singleTask"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="singleTask模式"/>

    <Button
        android:id="@+id/start_singleInstance"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="singleInstance模式"/>
</LinearLayout>